									<h2>{{bs.title}}</h2>
								</header>
							{% endif %}
							<style type="text/css">
								@media screen and (min-width:750px) {
									{{cssClass}} .header-block h2 {
										font-size: {{bs.title_size}}px;
									}
									{{cssClass}} .header-block h3 {
										font-size: {{bs.top_title_size}}px;
									}
								}
							</style>
						</div>
					{% endif %}

					{% if b.type == 'content' %}

					{% endif %}

					{% if b.type == 'quote' %}
						<div class="inner-container {{b.type}}-block ">
							{% if bs.title != blank  %}
								<header class="section-title">
									<h2>{{bs.title}}</h2>
								</header>
							{% endif %}
							{% if bs.text != blank %}
								<div class="rte">
									{% include 'icon-quote' %}
									{{bs.text}}
								</div>
							{% endif %}
							{% if bs.btn_txt != blank and bs.btn_url != blank %}
								<a class="btn " href="{{bs.btn_url}}">{{bs.btn_txt}}</a>
							{% endif %}
							<style type="text/css">
								@media screen and (min-width:750px) {
									{{cssClass}} .quote-block h2 {
										font-size: {{bs.text_size}}px !important;
									}
								}
								{% assign custom = bs.custom_html | raw %}
								{% assign cssLoop = custom | replace: '==', cssClass | replace: ';', ' !important;' %}
								{{cssLoop}}
							</style>
						</div>
					{% endif %}

					{% if b.type == 'text' %}
						<div class="inner-container {{b.type}}-block ">
							{% if bs.title != blank  %}
								<header class="section-title">
									<h2>{{bs.title}}</h2>
								</header>
							{% endif %}
							{% if bs.text != blank %}
								<div class="rte">{{bs.text}}</div>
							{% endif %}
							{% if bs.btn_txt != blank and bs.btn_url != blank %}
								<a class="btn " href="{{bs.btn_url}}">{{bs.btn_txt}}</a>
							{% endif %}
							<style type="text/css">
								@media screen and (min-width:750px) {
									{{cssClass}} .text-block h2 {
										font-size: {{bs.text_size}}px !important;
									}
									{% if bs.rte_text_size %}
									{{cssClass}} .text-block .rte {
										font-size: {{bs.rte_text_size}}px !important;
									}
									{% endif %}
								}
								{% assign custom = bs.custom_html | raw %}
								{% assign cssLoop = custom | replace: '==', cssClass | replace: ';', ' !important;' %}
								{{cssLoop}}
							</style>
						</div>
					{% endif %}

					{% if b.type == 'image' %}
